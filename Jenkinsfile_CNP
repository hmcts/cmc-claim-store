#!groovy

@Library("Infrastructure@feature/add-slack-integration")

def product = "cmc"
def component = "claim-store"

withPipeline("java", product, component) {
  after('checkout') {
    echo 'claim-store checked out'
  }

  onSuccess {
    echo '<<<<<< Success'
  }

  onFailure {
    echo '>>>>>> Failed'
  }

  enableSlackNotifications('#cmc-tech-notifications')
}
